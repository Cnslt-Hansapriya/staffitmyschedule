<mvc:View controllerName="mrs.resourcemanagement.view.assignment.CreateOtherBookingDialog" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
	<Page id="otherBookingPageId" title="{i18n>ASSIGNMENT_OTHER_NEW}" class="assignmentCreatePage">
		<content>
			<core:mvc.XMLView id="otherBooking" viewName="mrs.resourcemanagement.view.assignment.CreateOtherBooking" height="100%"/>
		</content>
		<footer>
			<Toolbar>
				<ToolbarSpacer/>
				<Button id = "btn1" text="Delete" press="deleteOtherAssignment"/>
				<Button text="{i18n>TEAM_SCHEDULE_ASSIGNMENT_CANCEL_BTN}" tooltip="{i18n>TEAM_SCHEDULE_ASSIGNMENT_CANCEL_BTN}" press="onCancelDialog"/>
				<Button text="{i18n>TEAM_SCHEDULE_ASSIGNMENT_CREATE_BTN}" tooltip="{i18n>TEAM_SCHEDULE_ASSIGNMENT_CREATE_BTN}" type="Emphasized"
					press="createOtherAssignment"/>
			</Toolbar>
		</footer>
	</Page>
</mvc:View>